<?xml version='1.0' encoding='utf-8'?>
<widget id="com.covidTrackerIndia.surajmandalcell" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Covid-19 India</name>
    <description>
        A covid-19 pandemic tracker for India.
    </description>
    <author email="cruelplatypus67@gmail.com" href="http://surajmandalcell.github.io">
        Surajmandalcell
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="www/assets/icons/icon-72x72.png" />
    <icon density="mdpi" src="www/assets/icons/icon-72x72.png" />
    <icon density="hdpi" src="www/assets/icons/icon-72x72.png" />
    <icon density="xhdpi" src="www/assets/icons/icon-96x96.png" />
    <icon density="xxhdpi" src="www/assets/icons/icon-144x144.png" />
    <icon density="xxxhdpi" src="www/assets/icons/icon-192x192.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
